{
  "name": "discord-voice-bot",
  "version": "1.0.0",
  "description": "A modular Discord voice bot with Speech-to-Text, LLM integration, and Text-to-Speech capabilities",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dm-call": "tsx src/dm-call-main.ts",
    "dm-call:dev": "tsx watch src/dm-call-main.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "discord",
    "voice",
    "bot",
    "speech-to-text",
    "text-to-speech",
    "typescript"
  ],
  "author": "Felix Schoberwalter <felix@schobidot.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schobiDotDev/discord-voice-bot.git"
  },
  "bugs": {
    "url": "https://github.com/schobiDotDev/discord-voice-bot/issues"
  },
  "homepage": "https://github.com/schobiDotDev/discord-voice-bot#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.18.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.1",
    "libsodium-wrappers": "^0.7.15",
    "onnxruntime-node": "^1.24.1",
    "opusscript": "^0.0.8",
    "prism-media": "^1.3.5",
    "undici": "^6.21.0",
    "winston": "^3.19.0",
    "ws": "^8.19.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.11.16",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
